[push]
  default       = upstream
[color]
  ui            = auto
[alias]
  a             = add
  aa            = add --all
  ap            = add --patch
  alias         = config --get-regexp ^alias\\.
  b             = checkout -b
  bb            = branch
  bd            = branch -d
  bdd           = branch -D
  bdjts         = ! git branch -D `git branch | grep -E 'jts-*'`
  c             = commit
  cc            = commit -am
  ca            = commit --amend
  cam           = commit -a
  can           = commit --amend --no-edit
  caan          = commit -a --amend --no-edit
  cap           = commit -p --amend
  caap          = commit -ap --amend
  cdate         = ! git commit -m `date "+%Y-%m-%d_%H:%M:%S" | awk '{ print $1 }'`
  cm            = commit -m
  caw           = commit -am "wip"
  cp            = commit -p
  cw            = commit -m "wip"
  cv            = commit -v
  co            = checkout
  d             = diff
  dh            = diff HEAD~1..HEAD
  dc            = diff --word-diff --cached --color-words
  df            = diff --word-diff --color-words
  l             = log --oneline
  ll            = log --oneline --decorate --graph -40
  lla           = log --oneline --decorate --graph --all -40
  llg           = log --oneline --decorate --graph --first-parent main -40
  lg            = log -E -i --grep
  lnumstat      = log --pretty=tformat: --numstat
  ls            = log --oneline
  lw            = log --date=relative --since='1 week ago' --oneline
  mup           = !git checkout main && git pull && git checkout -
  p             = push
  pf            = push --force-with-lease
  pfu           = push -fu origin HEAD
  pu            = push -u origin HEAD
  r             = rebase main
  ri            = rebase -i main
  ua            = reset
  uc            = reset --soft HEAD^
  ustage        = reset HEAD --
  us            = reset HEAD --
  undo          = co
[core]
	excludesfile  = /Users/johnschoeman/.gitignore_global
  autocrlf      = input
[merge]
  ff            = only
[commit]
	template      = ~/.gitmessage
[fetch]
  prune         = true
[rebase]
  autosquash    = true
[include]
  path          = ~/.gitconfig.local
[diff]
  colorMoved    = zebra
[user]
	name          = John Schoeman
	email         = johnschoeman1617@gmail.com
[http]
	postBuffer    = 1572864000
[init]
	defaultBranch = main
